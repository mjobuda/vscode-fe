{
    "name": "vscode-fe",
    "displayName": "Fe",
    "description": "Ethereum Fe language support for Visual Studio Code",
    "license": "MIT",
    "version": "0.0.11",
    "keywords": [
        "Fe",
        "ethereum",
        "blockchain",
        "compiler",
        "security"
    ],
    "publisher": "mjobuda",
    "engines": {
        "vscode": "^1.20.2"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "bugs": {
        "url": "https://github.com/mjobuda/vscode-fe/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mjobuda/vscode-fe"
    },
    "activationEvents": [
        "onLanguage:fe"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "themes": [
            {
                "label": "FeThemeDark",
                "uiTheme": "vs-dark",
                "path": "./themes/fetheme.json"
            }
        ],
        "languages": [
            {
                "id": "fe",
                "aliases": [
                    "Fe",
                    "fe"
                ],
                "extensions": [
                    ".fe"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "ast",
                "aliases": [
                    "AST",
                    "Ast",
                    "ast"
                ],
                "extensions": [
                    ".ast"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "yul",
                "aliases": [
                    "Yul",
                    "yul"
                ],
                "extensions": [
                    ".yul"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "tokens",
                "aliases": [
                    "Tokens",
                    "tokens"
                ],
                "extensions": [
                    ".tokens"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fe",
                "scopeName": "source.fe",
                "path": "./syntaxes/fe.tmLanguage.json"
            },
            {
                "language": "ast",
                "scopeName": "source.fe",
                "path": "./syntaxes/fe.tmLanguage.json"
            },
            {
                "language": "tokens",
                "scopeName": "source.fe",
                "path": "./syntaxes/fe.tmLanguage.json"
            },
            {
                "language": "yul",
                "scopeName": "source.fe",
                "path": "./syntaxes/fe.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "fe",
                "path": "./snippets/fe.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Fe (Ethereum)",
            "properties": {
                "fe.compile.onSave": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically compile when saving and annotate code with compile results."
                },
                "fe.mode.active": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable/Disable all active components of this extension (emergency)."
                },
                "fe.decoration.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether to enable/disable fe active syntax highlighting for security."
                },
                "fe.hover.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether to enable/disable fe tooltips/hover information."
                },
                "fe.command": {
                    "type": "string",
                    "default": "target/debug/fe",
                    "description": "Defines how to invoke the fe compiler command. This can by any shell command and the fe arguments will be passed to this command as <fe.command> <file> <fe.options>. Default is /target/debug/fe "
                },
                "fe.outputFolder": {
                    "type": "string",
                    "default": "output",
                    "description": "Defines the location of the compiled artifacts"
                },
                "fe.options": {
                    "type": "string",
                    "default": "--overwrite --emit=abi,bytecode,ast,tokens,yul,loweredAst",
                    "description": "Defines the options of Fe. Default is: --overwrite --emit=abi,bytecode,ast,tokens,yul,loweredAst"
                }
            }
        },
        "configurationDefaults": {
            "[fe]": {
                "editor.semanticHighlighting.enabled": true
            }
        },
        "tokenColors": [
            {
                "scope": "Def",
                "settings": {
                    "foreground": "#dd0000",
                    "fontStyle": "italic"
                }
            }
        ],
        "semanticTokenScopes": [
            {
                "language": "fe",
                "scopes": {
                    "Name": [
                        "variable.other.constant.property.ts"
                    ]
                }
            }
        ],
        "commands": [
            {
                "command": "fe.compileContract",
                "title": "fe: Compile"
            },
            {
                "command": "fe.openAST",
                "title": "Hello World"
            }
        ],
        "x-keybindings": [
            {
                "key": "cmd+alt+shift+i",
                "command": "editor.action.inspectTMScopes"
            }
        ],
        "keybindings": [
            {
                "command": "fe.openAST",
                "key": "f4"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
    },
    "dependencies": {
        "async": "^2.6.3",
        "chai": "^4.2.0",
        "mythxjs": "^1.3.1",
        "shell-escape": "^0.2.0",
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.42.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "eslint": "^7.21.0",
        "typescript": "^4.3.2"
    },
    "devDependencies": {
        "esbuild": "^0.12.14",
        "ts-loader": "^9.2.3",
        "webpack": "^5.42.0",
        "webpack-cli": "^4.7.2"
    }
}
